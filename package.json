{
  "name": "quick-directory",
  "version": "0.0.1",
  "description": "A productivity utility to quickly change directories",
  "bin": {
    "qd": "./qd.js",
    "hd": "./hd.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jrnewell/quick-directory.git"
  },
  "scripts": {
    "build":       "coffee --compile --output lib src",
    "clean":       "rm -rf lib",
    "prepublish":  "coffee --compile --output lib src",
    "postpublish": "rm -rf lib"
  },
  "dependencies" : {
    "commander": "latest",
    "chalk": "latest",
    "jsonfile": "latest",
    "lodash": "latest",
    "mkdirp": "latest"
  },
  "keywords": [
    "quick", "directory", "quick directory", "utility", "working", "working directory", "productivity"
  ],
  "author": "James Newell <james.newell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrnewell/quick-directory/issues"
  },
  "engines": {
    "node": ">=0.10",
    "npm": ">=1.4"
  }
}